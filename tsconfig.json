{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Navbar.jsx",
    "utils/motion.js",
    "sections/World.jsx",
    "sections/WhatsNew.jsx",
    "sections/Insights.jsx",
    "sections/Hero.jsx",
    "sections/GetStarted.jsx",
    "sections/Feedback.jsx",
    "sections/Explore.jsx",
    "sections/About.jsx",
    "components/StartSteps.jsx",
    "components/NewFeatures.jsx",
    "components/InsightCard.jsx",
    "components/Footer.jsx",
    "components/ExploreCard.jsx",
    "components/CustomTexts.jsx",
    "pages/_app.jsx",
    "pages/index.jsx"
  ],
  "exclude": ["node_modules"]
}
